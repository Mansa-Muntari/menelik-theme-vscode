    "tokenColors": [
        {
            "scope": [
                "variable",
                "entity.name",
                "entity.name.variable",
                "entity.other.attribute-name",
                "punctuation.separator",
                "punctuation.definition.tag",
                "punctuation.definition.list",
                "string.template meta.embedded",
                "support.variable",
                "storage.type.generic.java",
                "storage.modifier.import.java"
            ],
            "settings": {
                "foreground": "#D9D9D9"
            }
        },
        {
            "scope": [
                "string.quoted",
                "string.unquoted",
                "string.template",
                "support.constant.property-value.css",
                "meta.property-value.css"
            ],
            "settings": {
                "foreground": "#EFF0B7"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#595959",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "keyword", 
            "settings": {
                "foreground": "#85F4AC"
            }
        },
        {
            "scope": [
                "constant",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#FFBF50"
            }
        },
        {
            "scope": [
                "constant.character.escape",
                "keyword.operator",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.delete",
                "punctuation.separator.key-value.html",
                "punctuation.separator.inheritance.php"
            ],
            "settings": {
                "foreground": "#64E7E7"
            }
        }, 
        {
            "scope": [
                "punctuation.separator.scope-resolution",
                "punctuation.separator.scope-resolution.function.call",
                "punctuation.separator.namespace.access",
                "punctuation.separator.dictionary.key-value.json",
                "punctuation.separator.key-value.mapping.yaml"
            ],
            "settings": {
                "foreground": "#64E7E7",
                "fontStyle": "bold"
            }
        },       
        {
            "scope": [
                "entity.name.function",
                "entity.name.tag.yaml",
                "entity.other.attribute-name",
                "support.function",
                "support.type.property-name",
                "support.type.property-name.css",
                "meta.object-literal.key",
                "meta.function-call.generic"
            ],
            "settings": {
                "foreground": "#207560"
            }
        },
        {
            "scope": [
                "storage",
                "variable.language",
                "entity.other.attribute-name.id",
                "entity.other.attribute-name.pseudo-element",
                "storage.modifier.extends.java"
            ],
            "settings": {
                "foreground": "#64E7E7",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.tag",
                "entity.other.inherited-class",
                "entity.other.attribute-name.pseudo-class",
                "storage.type.cs",
                "storage.modifier.specifier",
                "storage.modifier",
                "support"
            ],
            "settings": {
                "foreground": "#DD9AC2"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#64704C",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#64704C"
            }
        },    
        {
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#85F4AC"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#E5E6B3",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#D9D9D9"
            }
        },
        {
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#DD9AC2"
            }
        },
        {
            "scope": "string.other.link",
            "settings": {
                "foreground": "#64E7E7"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "foreground": "#64704C",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "foreground": "#64704C",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.inline.raw",
                "punctuation.definition.markdown"
            ],
            "settings": {
                "foreground": "#207560"
            }
        },
        {
            "scope": "meta.separator.markdown",
            "settings": {
                "foreground": "#595959"
            }
        },
        {
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#61AAD1"
            }
        },
        {
            "scope": "punctuation.section.embedded",
            "settings": {
                "foreground": "#85F4AC"
            }
        }
    ]
}
